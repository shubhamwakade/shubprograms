   ],
   "source": [
    "samplelist=(eval(input(\"Entre The input:\")))\n",
    "def sum(x):\n",
    "    add=0\n",
    "    for i in x:\n",
    "        add+=i\n",
    "    return add        \n",
    "print('Expected Output: ',sum(samplelist))"
