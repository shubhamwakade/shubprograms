   "source": [
    "samplestring=input(\"Entre The string: \")\n",
    "lower=0\n",
    "upper=0\n",
    "for i in samplestring:\n",
    "    if(i.islower()):\n",
    "            lower+=1 \n",
    "    elif (i.isupper()):\n",
    "            upper+=1\n",
    "\n",
    "print(\"No. of Lower case Characters:\",lower)\n",
    "print(\"No. of Upper case characters:\",upper)"
   ]
