   "source": [
    "samplestring=input(\"Entre The Words: \")\n",
    "def reverse(samplestring):\n",
    "    str = \"\"\n",
    "    for i in samplestring:\n",
    "        str = i + str\n",
    "    return str\n",
    "print(\"The reverse output is: \",end=\"\")\n",
    "print(reverse(samplestring))"
